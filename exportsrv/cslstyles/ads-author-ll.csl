<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" and="text" initialize="false">
  <info>
    <title>ADS Author Custom Format l</title>
    <title-short>ADS Author Custom Format l</title-short>
    <id>https://ui.adsabs.harvard.edu</id>
    <category citation-format="author"/>
    <category field="generic-base"/>
    <summary>Format l: First Author has the format: lastname, f. i., Second Author on has the format: lastname, f. i., Before Last Author there is and symbol, and when shorten has the format: n author(s), et al.</summary>
    <updated>2017-07-12T18:07:14+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en"/>
  <macro name="author"/>
  <citation>
    <layout/>
  </citation>
  <bibliography and="symbol" initialize="true" entry-spacing="0" hanging-indent="true">
    <layout>
      <text macro="author"/>
      <names variable="author">
        <name and="symbol" delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="all"  delimiter="; "/>
        <label prefix=", "/>
      </names>
    </layout>
  </bibliography>
</style>

